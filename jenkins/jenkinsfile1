pipiline {
  agent none
    stages {
       agent any
          parallel {
             stage('BUILD') {
                 steps {
                      sh '''
                         #!/bin/bash
                           pwd 
                            ls
                           echo " this is build stage "
                           '''
                         }
                       }
                   stage('DEPLOY') {
                      steps {
                            echo " this is deploy stage "
                            }
                         }
                      }
                   }
               }
