pipeline {
   agent { label 'slave-maven'}
   stages {
        stage('BUILD') {
           steps {
                sh '''
                   #!/bin/bash
                     pwd
                     ls
                     echo " this is a BUILD stage"
                     sleep 5
                    '''
          }
        }
        stage('DEPLOY') {
           steps {
                  echo " this is a DEPLOY stage "
                  sh 'sleep 5'
          }
        }
        stage('TESTING1') {
           steps {
                 sh 'echo " this is testing1 stage"'
                 sh 'sleep 5'
           }
        }
        stage('TESTING2') {
           steps {
                sh '''
                  echo "this is testing2 stage"
                  sleep 5
                  '''
           
             }
       }
    }

}


